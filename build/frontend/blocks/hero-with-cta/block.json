{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "tailwind-starter/hero-with-cta",
  "version": "0.1.0",
  "title": "Hero with CTA",
  "category": "tailwind-starter",
  "icon": "star-filled",
  "description": "Create compelling hero sections with headlines, descriptions, and prominent call-to-action buttons.",
  "example": {},
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "layout": {
      "type": "string",
      "default": "centered"
    },
    "headline": {
      "type": "string",
      "default": "Transform Your Business with Our Solution"
    },
    "subheadline": {
      "type": "string",
      "default": "Discover how our innovative platform can help you achieve your goals faster and more efficiently than ever before."
    },
    "primaryCta": {
      "type": "object",
      "default": {
        "text": "Get Started",
        "url": "#",
        "opensInNewTab": false
      }
    },
    "secondaryCta": {
      "type": "object",
      "default": {
        "text": "Learn More",
        "url": "#",
        "opensInNewTab": false
      }
    },
    "showSecondaryCta": {
      "type": "boolean",
      "default": false
    },
    "backgroundImage": {
      "type": "object",
      "default": null
    },
    "backgroundVideo": {
      "type": "object",
      "default": null
    },
    "hasOverlay": {
      "type": "boolean",
      "default": false
    },
    "overlayOpacity": {
      "type": "number",
      "default": 50
    },
    "textAlignment": {
      "type": "string",
      "default": "center"
    },
    "visualSettings": {
      "type": "object",
      "default": {}
    }
  },
  "textdomain": "tailwind-starter",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}