{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "768px",
      "wideSize": "1200px"
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "size": "1rem",
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "1.5rem",
          "slug": "medium",
          "name": "Medium"
        },
        {
          "size": "2rem",
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "3rem",
          "slug": "x-large",
          "name": "Extra Large"
        }
      ],
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Inter, system-ui, sans-serif",
          "name": "Inter",
          "slug": "inter"
        },
        {
          "fontFamily": "Poppins, system-ui, sans-serif",
          "name": "Poppins",
          "slug": "poppins"
        }
      ],
      "fontSizes": [
        {
          "size": "0.875rem",
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "1rem",
          "slug": "medium",
          "name": "Medium"
        },
        {
          "size": "1.125rem",
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "1.25rem",
          "slug": "x-large",
          "name": "Extra Large"
        },
        {
          "size": "1.5rem",
          "slug": "xx-large",
          "name": "XX Large"
        }
      ],
      "lineHeight": true,
      "letterSpacing": true
    },
    "color": {
      "palette": [
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#f9fafb",
          "name": "Gray 50",
          "slug": "gray-50"
        },
        {
          "color": "#6b7280",
          "name": "Gray 500",
          "slug": "gray-500"
        },
        {
          "color": "#111827",
          "name": "Gray 900",
          "slug": "gray-900"
        },
        {
          "color": "#0ea5e9",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#ec4899",
          "name": "Secondary",
          "slug": "secondary"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(135deg, #0ea5e9 0%, #ec4899 100%)",
          "name": "Primary to Secondary",
          "slug": "primary-secondary"
        }
      ],
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "link": true
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        },
        "color": {
          "background": true,
          "text": true
        },
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSize": true,
          "fontWeight": true
        }
      },
      "core/heading": {
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Poppins, system-ui, sans-serif",
              "name": "Poppins",
              "slug": "poppins"
            }
          ]
        }
      },
      "core/paragraph": {
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Inter, system-ui, sans-serif",
              "name": "Inter",
              "slug": "inter"
            }
          ]
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--gray-900)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontWeight": "600",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "2.5rem"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "2rem"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "1.5rem"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "0.375rem"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "spacing": {
          "padding": {
            "top": "0.75rem",
            "right": "1.5rem",
            "bottom": "0.75rem",
            "left": "1.5rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "500"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "1.5rem",
          "fontWeight": "700"
        }
      }
    }
  }
}