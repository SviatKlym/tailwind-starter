{
  "name": "tailwind-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run blocks:build && npm run theme:build",
    "build:analyze": "npm run build",
    "preview": "vite preview",
    "blocks:build": "wp-scripts build",
    "blocks:dev": "wp-scripts start",
    "theme:build": "tailwindcss -i ./src/style.css -o ./build/main.css --minify",
    "performance:audit": "npm run build",
    "clean": "rimraf build dist"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-react": "^4.7.0",
    "@wordpress/scripts": "^30.19.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.5",
    "rimraf": "^5.0.1"
  },
  "dependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.45.1",
    "@wordpress/block-editor": "^14.4.0",
    "@wordpress/blocks": "^13.9.0",
    "@wordpress/components": "^28.9.0",
    "@wordpress/compose": "^7.9.0",
    "@wordpress/element": "^6.9.0",
    "@wordpress/hooks": "^4.9.0",
    "@wordpress/i18n": "^5.9.0",
    "ajv": "^8.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
